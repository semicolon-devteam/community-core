    "@tanstack/react-query": "^5.69.0",
    "@toast-ui/react-editor": "^3.2.3",
    "axios": "^1.8.1",
    "cookies-next": "^6.1.0",
    "lodash": "^4.17.21",
    "next": "15.1.4",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-redux": "^9.2.0",
    "react-simple-captcha": "^9.3.1",
    "react-spinners": "^0.17.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
