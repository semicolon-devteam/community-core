    "@vitest/ui": "^3.1.2",
    "eslint": "^8",
    "eslint-config-next": "15.0.3",
    "jsdom": "^26.1.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "vitest": "^3.1.4"
